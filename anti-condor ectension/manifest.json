{
  "manifest_version": 3,
  "name": "Phishing URL Detector",
  "version": "1.0",
  "description": "Detect phishing URLs using a Machine Learning model.",
  "permissions": ["activeTab", "storage", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://127.0.0.1:5000/*" 
  ],
  "action": {
    "default_popup": "popup.html"
    
  }
}
